{
  "version": "1.0.0-*",
  "testRunner": "xunit",
  "buildOptions": {
    "copyToOutput": {
      "include": [
        "SerializationTests/ConfigurationTests/*.xml",
        "DeploymentTests/Configuration/**/*.xml"
      ]
    }
  },
  "dependencies": {
    "Orleans": { "target": "project" },
    "ClientGenerator": { "target": "project" },
    "OrleansAzureUtils": { "target": "project" },
    "OrleansBondUtils": { "target": "project" },
    "OrleansCodeGenerator": { "target": "project" },
    "OrleansGoogleUtils": { "target": "project" },
    "OrleansManager": { "target": "project" },
    "OrleansProviders": { "target": "project" },
    "OrleansRuntime": { "target": "project" },
    "OrleansServiceBus": { "target": "project" },
    "OrleansSQLUtils": { "target": "project" },
    "OrleansTestingHost": { "target": "project" },
    "TestExtensions": { "target": "project" },
    "TestFSharp": { "target": "project" },
    "TestFSharpInterfaces": { "target": "project" },
    "TestGrainInterfaces": { "target": "project" },
    "TestGrains": { "target": "project" },
    "TestInterfaces": { "target": "project" },
    "TestInternalGrainInterfaces": { "target": "project" },
    "TestInternalGrains": { "target": "project" },
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "FluentAssertions": "4.0.0",
    "WindowsAzure.Storage": "7.0.0",
    "xunit": "2.2.0-beta2-build3300",
    "Xunit.SkippableFact": "1.2.14"
  },
  "frameworks": {
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETSTANDARD" ],
        "compile": { "exclude": [ "DeploymentTests/**" ] }
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    },
    "net451": {
      "buildOptions": {
        "compile": {
          "exclude": [ "PSClientTests/**", "**/SQS*" ]
        }
      },
      "dependencies": {
        "System.Management.Automation": "6.1.7601.17515"
      },      "frameworkAssemblies": {
        "System.Management": "4.0.0.0"
      }
    }
  }
}
